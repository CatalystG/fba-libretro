CELL_DEBUG         = 0
CELL_DEBUG_CONSOLE = 0
CELL_DEBUG_MEMORY  = 0
MULTIMAN_SUPPORT   = 0
DOWNLOAD_SHADERS	= 1

CELL_BUILD_TOOLS = GCC

ifeq ($(CELL_DEBUG),1)
PPU_OPTIMIZE_LV      := -O0
CELL_DEBUG_DEFINES   := -g
else
PPU_OPTIMIZE_LV      := -O3
CELL_DEBUG_DEFINES   := -DNDEBUG
endif

